  <?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
	<worksheet name="Hoja4">
		<loop startRow="1" endRow="1" items="ordenes.orden" var="orden"
			varType="org.gaviot.transformer.model.progreso.Orden">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">orden.ordenId</mapping>
				<mapping row="1" col="1">orden.solicitud</mapping>
				<mapping row="1" col="2">orden.apellidoNombre</mapping>
				<mapping row="1" col="3">orden.domicilio</mapping>
				<mapping row="1" col="4">orden.localidad</mapping>
				<mapping row="1" col="5">orden.aseguradora</mapping>
				<mapping row="1" col="6">orden.bienAsegurado</mapping>
				<mapping row="1" col="7">orden.riesgoACubrir</mapping>
				<mapping row="1" col="8">orden.sumaAsegurada</mapping>
				<mapping row="1" col="9">orden.vigencia</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0"></cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>
                